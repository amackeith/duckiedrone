FROM ros:kinetic-ros-base

MAINTAINER Arthur MacKeith <amackeith@uchicago.edu>

#this installs ping, vim, and sshfs
RUN apt-get update && apt-get install -y iputils-ping && apt-get install -y vim && apt-get install -y sshfs

RUN apt-get install -y screen

#this is the begining of setting up ssh (I just copied it so idk about THEPASSWORDYOUCREATED)
RUN apt-get update && apt-get install -y openssh-server
RUN mkdir /var/run/sshd
RUN echo 'root:THEPASSWORDYOUCREATED' | chpasswd
RUN sed -i 's/PermitRootLogin prohibit-password/PermitRootLogin yes/' /etc/ssh/sshd_config

# SSH login fix. Otherwise user is kicked off after login
RUN sed 's@session\s*required\s*pam_loginuid.so@session optional pam_loginuid.so@g' -i /etc/pam.d/sshd

ENV NOTVISIBLE "in users profile"
RUN echo "export VISIBLE=now" >> /etc/profile

EXPOSE 22
CMD ["/usr/sbin/sshd", "-D"]
#this is the end of setting up ssh

COPY otherfiles/.bashrc ~/

RUN mkdir -p ~/ws/src/pidrone_pkg
COPY pidrone_pkg19 ~/ws/src/pidrone_pkg
RUN /bin/bash -c '. /opt/ros/kinetic/setup.bash; cd ~/ws; catkin_make'

